        mode_heading = "text-2xl md:text-xl lg:text-2xl xl:text-3xl"
        section = " p-6 my-8 mx-auto w-[95vw] max-w-[95vw] md:w-[1200px] text-black bg-zinc-200 rounded-lg text-left"

doctype html
html lang="en"

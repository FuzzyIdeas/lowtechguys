                icon.w-16.h-16.md:w-20.md:h-20.filter.drop-shadow-lg.text-zinc-600 : lowtech
            .links
                a.text-sm.md:text-base.ml-4.hidden.md:inline href="/studioicc" StudioICC
                a.text-sm.md:text-base.ml-4 href="/studioicc/privacy" Privacy

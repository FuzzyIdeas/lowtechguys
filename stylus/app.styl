@tailwind base
@tailwind components
@tailwind utilities

@import base
@import animations
@import fonts
@import modernize

.layout-container
    margin 0
    padding 0

.spin-alternate-fast
    animation spin 1.5s cubic-bezier(0, 0.4, 0, 0.89) alternate infinite

.spin-fast
    animation spin 1s linear infinite

.spin
    animation spin 1.5s linear infinite

.no-anim
    animation none

.headline
    font-family headlineFont !important

.small-caps
    font-variant small-caps

h1
    font-weight: theme('fontWeight.bold')
    font-size: theme('fontSize.3xl')
    color: theme('textColor.white')

h2
    font-weight: theme('fontWeight.bold')
    font-size: theme('fontSize.2xl')
    color: theme('textColor.white')
    +mobile()
        font-size: theme('fontSize.xl')

h3
    font-weight: theme('fontWeight.bold')
    font-size: theme('fontSize.xl')
    color: theme('textColor.white')
    +mobile()
        font-size: theme('fontSize.lg')

h4
    font-weight: theme('fontWeight.bold')
    font-size: theme('fontSize.lg')
    color: theme('textColor.white')
    +mobile()
        font-size: theme('fontSize.base')

a
    font-weight: theme('fontWeight.medium')
    color: theme('textColor.red.dull')
    transition-duration: theme('transitionDuration.200')
    transition-property: theme('transitionProperty.all')
    text-decoration-line: underline
    text-decoration-thickness: 1px
    text-decoration-color: currentColor

    &:hover
        color: theme('textColor.red.hot')
        text-decoration-line: underline
        text-decoration-thickness: 2px
        text-decoration-color: currentColor

.links
    a, a:hover, a:focus
        text-decoration none !important

.btn
    font-weight: theme('fontWeight.medium')
    border-radius: theme('borderRadius.2xl')
    transition-duration: theme('transitionDuration.150')
    transition-property: theme('transitionProperty.DEFAULT')
    padding: theme('padding.3') theme('padding.5')
    text-decoration none !important


body section#hero .btn-paddle,
body section#pro .btn-paddle
    font-size: 15px !important
    font-family: sansSerif !important
    font-weight: theme('fontWeight.medium') !important
    border-radius: theme('borderRadius.DEFAULT') !important
    transition-duration: theme('transitionDuration.200') !important
    transition-property: theme('transitionProperty.DEFAULT') !important
    padding: 0.9rem theme('padding.6') !important
    color: theme('textColor.black') !important
    background: theme('backgroundColor.white') !important
    display: flex !important
    justify-content center !important
    align-items center !important
    box-shadow: theme('boxShadow.lg') !important
    border: none !important

    &:hover
        border: none !important
        box-shadow: theme('boxShadow.2xl') !important
        color: theme('textColor.yellow.bright') !important
        background: theme('backgroundColor.mauve.blackish') !important

.btn-white
    background-color: theme('backgroundColor.white')
    color: theme('textColor.black')

.btn-white:hover
    background-color: theme('backgroundColor.yellow.lunar')
    background-color: theme('backgroundColor.yellow.lunarP3')
    color: theme('textColor.mauve.lightgray')

    svg
        filter sepia(1)

.btn-yellow-lunar
    background-color: theme('backgroundColor.yellow.lunar')
    color: theme('textColor.black')

.btn-yellow-lunar:hover
    background-color: theme('backgroundColor.mauve.black')
    color: theme('textColor.white')

.btn-yellow-bright
    background-color: theme('backgroundColor.yellow.bright')
    color: theme('textColor.black')

.btn-yellow-bright:hover
    background-color: theme('backgroundColor.amber.400')
    color: theme('textColor.black')

.btn-yellow-peach
    background-color: theme('backgroundColor.yellow.peach')
    color: theme('textColor.black')

.btn-yellow-peach:hover
    background-color: theme('backgroundColor.yellow.lunar')
    color: theme('textColor.black')

.btn-orange-dot
    background-color: theme('backgroundColor.orange.dot')
    color: theme('textColor.black')

.btn-orange-dot:hover
    background-color: theme('backgroundColor.white')
    color: theme('textColor.black')

.btn-orange-bmac
    background-color: theme('backgroundColor.orange.bmac')
    color: theme('textColor.white')

.btn-orange-bmac:hover
    background-color: theme('backgroundColor.white')
    color: theme('textColor.black')

.flex-center
    display: flex
    justify-content: center
    align-items: center

.flex-col-center
    display: flex
    flex-direction: column
    justify-content: center
    align-items: center

.monospaced
    font-family: secondaryFont !important

for colorName, colorValue in colors
    .glow-{colorName}
        soft-shadow colorValue 0.4 1 0.5

    .glow-text-{colorName}
        soft-text-shadow colorValue 0.4 1 0.5

    .glow-drop-{colorName}
        soft-drop-shadow colorValue 0.4 1 0.5

ul
    list-style-type disc
    list-style-position inside
    margin: revert
    padding: revert

ol
    list-style-type decimal
    list-style-position inside
    margin: revert
    padding: revert

p
    margin-top 0.5rem
strong
    color theme('colors.red.500')

.tooltip
    visibility hidden
    position absolute

.has-tooltip:hover .tooltip
    visibility visible
    z-index 100

.has-tooltip-manual.show-tooltip .tooltip
    visibility visible
    z-index 100


#answers
#releases
    .ais-Hits-list
        list-style none
        margin 0
        padding 0
    mark
        background sunflower
        color darkMauve
        border-radius 2px
        font-weight bold

#searchbox
    margin-bottom 1rem

    .ais-SearchBox-form
        display flex
        background lunarYellow
        border-radius 8px
        soft-shadow pitchBlack 0.5 1 2
        padding 1rem 2rem
        max-width 90vw
    button
        margin 0 0.5rem
    input
        background transparent
        width 500px
        max-width 80vw
        outline none
        color mauve
        font-weight 500

        &::placeholder
            color lightGrayMauve
            opacity 0.7

code
    background-color lightGrayMauve
    padding 2px 4px
    border-radius 8px
    border solid 2px alpha(warmWhite, 30%)
    font-weight 600
    color lunarYellow

pre code
    background-color transparent
    padding 0
    border-radius 0
    border none

pre
    background mix(black, darkMauve, 50%)
    padding 1rem
    margin 1rem auto
    border-radius 8px
    overflow-x auto

.codehilite
    padding 0.2rem 1rem
    margin 1rem auto
    border-radius 8px
    overflow-x auto

    pre
        background transparent
        padding revert
        margin revert
        border-radius revert
        overflow-x revert
        color white


.content
    transition-property: max-height
    transition-timing-function: ease-out
    transition-duration: 150ms

hr
    border-color marsRed
    margin 0.8rem 0

em
    color peach
h4
    color grayBlue

h3
    color warmWhite
    margin-top 1.5rem

video
    margin 1rem auto
    border-radius 8px
    soft-shadow pitchBlack 0.3 1 2

.links
    a
        background-color alpha(white, 0.5)
        color blackGray
        border-radius 8px
        padding 0.5rem 1rem
        word-break keep-all
        white-space nowrap
        +mobile()
            padding 0.35rem 0.7rem

        &:hover
        &:focus
            background-color alpha(white, 0.8)
            color black

.zstack
    display: grid
    align-items: center
    justify-items: center

.zstack > *
    grid-area: 1/1/1/1


#volum-knob
    position relative
    background: center / contain no-repeat linear-gradient(rgba(255, 255, 255, 0.6), rgba(255, 255, 255, 0.4)), conic-gradient(from 1.5708rad at 50% 50%, #F1CEA3 2%, #E3BE93 4%, #CA9F75 17%, #E6C295 21%, #E1B68B 27%, #DFB287 31%, #F1CEA2 37%, #F6D7AB 41%, #C49E80 54%, #D0A580 65%, #C39B7F 73%, #F5D8AC 83%, #F5D8AB 88%, #DEB085 95%)
    border-radius: 9999px
    margin-top: -2%

    #volum-knob-border
        width: 100%
        height: 100%
        border: 2px solid rgba(255, 255, 255, 0.4)
        border-radius: 9999px

    #volum-knob-indicator
        width: 10%
        height: 10%
        border-radius: 9999px
        background #888
        position absolute
        bottom 24%
        left 12%

.anchor-headings
    margin: 0 5px
    font-weight: bold
    font-size: smaller
    font-family: monoSpace
    color: lunarYellow
    opacity: 0.5
    &:hover
        opacity: 1

    &:visited
        color: peach

    &:active
        color: dullRed

body section#hero .btn-paddle,
body section#pro .btn-paddle
    font-size: 16px !important
    font-family: sansSerif !important
    font-weight: theme('fontWeight.medium') !important
    border-radius: theme('borderRadius.xl') !important
    transition-duration: theme('transitionDuration.150') !important
    transition-property: theme('transitionProperty.all') !important
    padding: 0 theme('padding.4') !important
    color: theme('textColor.white') !important
    background: theme('backgroundColor.zinc.700') !important
    display: flex !important
    justify-content center !important
    align-items center !important
    box-shadow: theme('boxShadow.lg') !important
    border: none !important
    text-decoration: none !important

    &:hover
        border: none !important
        box-shadow: theme('boxShadow.2xl') !important
        color: theme('textColor.white') !important
        background: theme('backgroundColor.red.500') !important

body section#pro .btn-paddle
    color: theme('textColor.white') !important
    background: theme('backgroundColor.black') !important

.paper
    --paper-bg-url: url("data:image/webp;base64,")


.paper-bg
    background: center/cover repeat linear-gradient(0deg, alpha(desaturate(lighten(niceYellow, 40%), 25%), 0.62), alpha(desaturate(lighten(niceYellow, 40%), 25%), 0.62)), var(--paper-bg-url)
    @media screen and (prefers-color-scheme: dark)
        background: center/cover repeat linear-gradient(0deg, hsla(336, 5%, 16%, 0.82), cubic-bezier(0.48, 0.3, 0.64, 1), hsla(24, 17%, 23%, 0.82)),var(--paper-bg-url)

.paper-bg-serious
    background: center/cover repeat linear-gradient(0deg, hsla(336, 5%, 16%, 0.82), cubic-bezier(0.48, 0.3, 0.64, 1), hsla(24, 17%, 23%, 0.82)),var(--paper-bg-url)
